b = int(input("Введите одно шестизначное целое число - номер билета: "))
a1 = b // 100000
a2 = b % 100000 // 10000
a3 = b % 10000 // 1000
a4 = b % 1000 // 100
a5 = b % 100 // 10
a6 = b % 10

if a1 + a2 + a3 == a4 + a5 + a6:
    print("Билет ", "b", " счастливый!")
else:
    print("Билет ", "b", " Не счастливый!")
